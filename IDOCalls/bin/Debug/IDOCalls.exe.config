<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="IDOWebServiceSoap" maxReceivedMessageSize = "10000000" />
            </basicHttpBinding>
            <customBinding>
                <binding name="IDOWebServiceSoap12" maxReceivedMessageSize = "10000000" >
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://grcdslutil/IDORequestService/IDOWebService.asmx"
                binding="basicHttpBinding" bindingConfiguration="IDOWebServiceSoap"
                contract="IDOWebServiceReference.IDOWebServiceSoap" name="IDOWebServiceSoap" />
            <endpoint address="http://grcdslutil/IDORequestService/IDOWebService.asmx"
                binding="customBinding" bindingConfiguration="IDOWebServiceSoap12"
                contract="IDOWebServiceReference.IDOWebServiceSoap" name="IDOWebServiceSoap12" />
        </client>
    </system.serviceModel>
</configuration>
